{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
     "paths": {
      "@theme": [
        "src/theme"
      ],
      "@atoms/*": [
        "src/components/atoms/*"
      ],
      "@molecules/*": [
        "src/components/molecules/*"
      ],
      "@organisms/*": [
        "src/components/organisms/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@assets": [
        "src/assets"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@containers/*": [
        "src/containers/*"
      ],
      "@navigation": [
        "src/navigation"
      ],
    }
  }
}
